{
  "presets": [
    [
      "env", { 
        "modules": false 
      }
    ],
    "react",
    "stage-2"
  ],
  "env": {
    "development": {
      "plugins": [ "react-hot-loader/babel" ]
    },
    "production": {
      "plugins": [ "transform-react-remove-prop-types" ]
    },
    "test": {
      "presets": [ "env", "react", "stage-2" ],
      "plugins": [ "transform-react-remove-prop-types" ]
    }
  }
}
